                "hvb",
                "hisysevent",
                "hiprofiler",
                "bounds_checking_function",
                "bundle_framework",
                "selinux",
                "selinux_adapter",
                "storage_service",
                "mbedtls",
                "zlib",
                "cJSON",
                "mksh",
                "toybox",
                "hicollie",
                "drivers_interface_partitionslot",
                "code_signature",
                "runtime_core"
            ],
            "third_party": [
                "mksh",
                "e2fsprogs",
                "f2fs-tools"
            ]
        },
        "build": {
            "group_type": {
                "base_group": [
                ],
                "fwk_group": [
                    "//base/startup/init:init_fwk_group"
                ],
